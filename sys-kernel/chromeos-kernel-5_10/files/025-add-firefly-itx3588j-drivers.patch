+	LINEIN_TYPE0,    //ITX-3588J USE
+	LINEIN_TYPE1,    //ROC-RK3588S-PC USE
+	LINEIN_TYPE2,    //AIO-3588SJD4 USE
+	LINEIN_TYPE3,    //ROC-RK3588-PC USE
+};
