+static int xc7160_configure_regulators(struct xc7160 *xc7160)
+{
+	unsigned int i;
+
+	for (i = 0; i < XC7160_NUM_SUPPLIES; i++)
+		xc7160->supplies[i].supply = xc7160_supply_names[i];
+
+	return devm_regulator_bulk_get(&xc7160->client->dev,
+				       XC7160_NUM_SUPPLIES,
+				       xc7160->supplies);
+}
